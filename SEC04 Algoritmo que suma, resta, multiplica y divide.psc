Algoritmo SEC04
	Definir A, B Como Entero;
	Definir C, D, F, G Como Real;
	Escribir "Ingresa un número:";
	Leer A;
	Escribir "Ingresa otro número:";
	Leer B;
	//Realizar las operaciones
	C <- A+B
	D <- A-B
	F <- A*B
	G <- A/B
	//Mostrar en la pantalla final el resultado de las operaciones
	Escribir "Los dos numeros son:", A "y" B;
	Escribir "La suma es:", C;
	Escribir "La resta es:", D;
	Escribir "La multiplicación es:", F;
	Escribir "La división es:", G;
FinAlgoritmo
